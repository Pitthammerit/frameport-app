NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Cloudflare R2 Configuration
CLOUDFLARE_R2_ACCESS_KEY_ID=your-access-key-id-here
CLOUDFLARE_R2_SECRET_ACCESS_KEY=your-secret-access-key-here
CLOUDFLARE_R2_BUCKET_NAME=frameport
CLOUDFLARE_R2_ENDPOINT=https://a9d607f4223071c884434e2efd48f485.r2.cloudflarestorage.com
CLOUDFLARE_ACCOUNT_ID=a9d607f4223071c884434e2efd48f485

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME="Frameport App"

# Email Configuration (via Supabase)
EMAIL_FROM=noreply@yourdomain.com
EMAIL_REPLY_TO=support@yourdomain.com

# Security
JWT_SECRET=your_jwt_secret_key
